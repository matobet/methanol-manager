<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='methanolmanager'>
  <!-- PWT Module to extends in order to use PWT widgets-->
  <inherits name="fr.putnami.pwt.core.inject.Inject" />
  <inherits name="fr.putnami.pwt.core.model.Model" />
  <inherits name="fr.putnami.pwt.core.mvp.Mvp" />
  <inherits name="fr.putnami.pwt.core.widget.Widget"/>

  <entry-point class="cz.muni.fi.pa165.methanolmanager.frontend.client.GwtApp"/>

  <source path="client" />
  <source path="shared" />

  <!-- This linker will be the default one in later version of GWT -->
  <add-linker name="xsiframe" />

  <set-configuration-property name="devModeRedirectEnabled" value="true" />

  <set-property name="compiler.useSourceMaps" value="true" />

  <collapse-all-properties />

</module>